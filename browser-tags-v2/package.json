{
  "name": "depict-ai",
  "version": "0.0.1",
  "description": "Browser tag",
  "repository": "depict.ai",
  "author": "hello@depict.ai",
  "dev": "depict-ai.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "serve": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/serve $X'",
    "build": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/build_prod $X'",
    "test-production-build": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/test_production_build $X'",
    "//": "Runs 'yarn && yarn build' in the defined order in the packages in packages/, add new packages here in the correct order to make them work in CI",
    "build_packages": "node --version && yarn clean && cd ./packages && bash -c 'for package in utilishared performance-client ui js-ui react-ui portal-components; do pushd \"$package\"; yarn build; popd; done'",
    "fast_build_packages": "yarn clean && zsh -c 'for package in utilishared performance-client ui js-ui react-ui portal-components; do (cd \"packages/$package\" && env -i TERM=\"$TERM\" PATH=\"$PATH\" bash -c \"yarn build\") & done; wait'",
    "reverse_proxy": "X=\"$@\" bash -c 'cd $PROJECT_CWD && dotenv -e .env.development -- ./build_scripts/to_reverse_proxy $X'",
    "demo": "X=\"$@\" bash -c 'cd $PROJECT_CWD && dotenv -e .env.development -- ./build_scripts/to_reverse_proxy $X'",
    "mkdemo": "ssh root@demo-machine.europe-west1-b.depict-v2 one_click_demo",
    "clean": "X=\"$@\" bash -c 'cd $PROJECT_CWD && rm -rf .parcel-cache .cache dist dev_dist $X'",
    "fresh-start": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/fresh-start $X'",
    "format": "prettier --write ./{src,lib,packages,devtools,tsconfig.json,testcafe,build_scripts,.yarnrc.yml}",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint --fix .",
    "deploy": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/deploy_rebuild -c 600 $X'",
    "deploy_no_rebuild": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/deploy_no_rebuild -c 600 $X'",
    "oneclick_demo": "X=\"$@\" bash -c 'cd $PROJECT_CWD && ./build_scripts/deploy_oneclick $X'",
    "generate_loader": "X=\\\"$@\\\" bash -c 'cd $PROJECT_CWD && devtools/generate_loader $X'",
    "generate_shopify_loader": "X=\\\"$@\\\" bash -c 'cd $PROJECT_CWD && devtools/generate_shopify_loader $X'",
    "g:tsc": "cd $INIT_CWD && tsc",
    "watch_packages": "$PROJECT_CWD/devtools/watch_packages.sh",
    "watch_ui_package": "$PROJECT_CWD/devtools/watch_packages.sh ui",
    "watch_utilishared_package": "$PROJECT_CWD/devtools/watch_packages.sh utilishared",
    "watch_react_ui_package": "$PROJECT_CWD/devtools/watch_packages.sh react-ui",
    "unit-test-ci": "jest --ci --config jest.config.json",
    "publish-packages-dev": "$PROJECT_CWD/devtools/publish_packages.sh patch dev",
    "publish-packages-latest": "$PROJECT_CWD/devtools/publish_packages.sh patch latest",
    "build_scss_docs": "cd packages/plp-styling && sassdoc .",
    "bypass-package-compilation": "yarn clean && node devtools/bypass-package-compilation.mjs",
    "bypass-package-compilation-vite": "cd .. && git apply browser-tags-v2/devtools/sdk-no-compilation-vite-changes-required.patch",
    "dangerous-revert-bypass-package-compilation": "git restore 'packages/*/package.json' 'packages/*/locales/latest/index.d.ts' 'packages/*/latest/index.d.ts' 'babel.config.js' && cd .. && git apply browser-tags-v2/devtools/revert-sdk-no-compilation-vite-changes-required.patch",
    "test-storefront": "./devtools/test-storefront"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": true
  },
  "alias": {
    "process": {
      "global": "process"
    }
  },
  "targets": {
    "modern": {
      "engines": {
        "browsers": "firefox 77, safari 13.1, ios 13.4"
      }
    },
    "ie11": {
      "engines": {
        "browsers": "ie 11"
      }
    },
    "edge": {
      "engines": {
        "browsers": "edge 17"
      }
    },
    "dev": {
      "engines": {
        "browsers": "last 1 firefox version"
      }
    }
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "resolutions": {
    "ip": "patch:ip@npm:1.1.5#./.yarn/patches/ip-npm-1.1.5-af36318aa6.patch",
    "htmlnano": "2.0.3",
    "@parcel/workers@2.9.3": "patch:@parcel/workers@npm%3A2.9.3#./.yarn/patches/@parcel-workers-npm-2.9.3-b0559c8ccb.patch"
  },
  "packageManager": "yarn@4.5.3",
  "devDependencies": {
    "@babel/cli": "latest",
